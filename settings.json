{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      // 在 wt 开启的时候，可以通过，alt+t 快速打开 wt
      "command": {
        "action": "globalSummon"
      },
      "keys": "alt+t"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    }
  ],
  "alwaysShowNotificationIcon": true,
  "autoHideWindow": true,
  "copyFormatting": "none",
  "copyOnSelect": false,
  "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
  "disableAnimations": false,
  "experimental.rendering.forceFullRepaint": false,
  "focusFollowMouse": true,
  "initialCols": 140, // 大小
  "initialPosition": "240,45", // 居中位置
  "initialRows": 40,
  "language": "en-US",
  "launchMode": "focus", // 专注模式启动
  "minimizeToNotificationArea": true,
  "profiles": {
    "defaults": {
      "adjustIndistinguishableColors": "never",
      "antialiasingMode": "grayscale",
      "backgroundImageOpacity": 0.2,
      "bellStyle": "audible",
      "colorScheme": "Harper",
      "cursorShape": "bar", // 指针样式
      "font": {
        "face": "FiraCode NF",
        "weight": "black"
      },
      "historySize": 1000,
      "intenseTextStyle": "all",
      "opacity": 50,
      "padding": "50",
      "scrollbarState": "hidden",
      "startingDirectory": "%USERPROFILE%",
      "useAcrylic": true,
      "useAtlasEngine": true
    },
